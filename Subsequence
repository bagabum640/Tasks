using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Task2
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int firstValue;
            int maxValue;
            int step;
               
            firstValue = 5;
            maxValue = 96;
            step = 7;

            for (int i = firstValue; i <= maxValue; i+=step)
            {
                Console.WriteLine(i);
            }
        }
    }
}
