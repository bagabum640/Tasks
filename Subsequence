using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Task2
{
    internal class Program
    {
        static void Main(string[] args)
        {            
            int maxValue;
            int step;
                       
            maxValue = 96;
            step = 7;

            for (int i = 5; i <= maxValue; i+=step)
            {
                Console.WriteLine(i);
            }
        }
    }
}
